				<ProtectedString name="Source"><![CDATA[local PLUGIN = {}
PLUGIN.Name = "RobloxGPT"
PLUGIN.ToolBar = plugin:CreateToolbar(PLUGIN.Name)
PLUGIN.Buttons = {
	RobloxGPT = {
		Button = PLUGIN.ToolBar:CreateButton("RobloxGPT", "ChatGPT as a roblox plugin.", "rbxassetid://12539250892"),
		GUI = script.Parent.RobloxGPT.RobloxGPT,
		StartUp = function(ButtonTable)
			if not game:GetService("Players").LocalPlayer then return end
			local Thumbnail = game:GetService("Players"):GetUserThumbnailAsync(game:GetService("Players").LocalPlayer.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size100x100)
			local themes = require(script.Parent.Themes)
			local Writing = false
			local settings = plugin:GetSetting("RobloxGPT") or {}
			settings.Mode = settings.Mode or "Dark"
			settings.Conversations = settings.Conversations or {}
			settings.CurrentConversation = settings.CurrentConversation
			settings.API = settings.API
			
			ButtonTable.Button.ClickableWhenViewportHidden = true
			ButtonTable.Connections, ButtonTable.RAM = {}, {
				Activated = false
			}

			ButtonTable.DockWidgetDestroyed = function()
				ButtonTable.DockWidget.Enabled = false
				ButtonTable.RAM.Activated = false
			end
			
			ButtonTable.Connections.ButtonClick = ButtonTable.Button.Click:Connect(function()
				ButtonTable.RAM.Activated = not ButtonTable.RAM.Activated

				if ButtonTable.RAM.Activated then
					if not ButtonTable.DockWidget then
						local widgetInfo = DockWidgetPluginGuiInfo.new(
							Enum.InitialDockState.Left,
							true,
							true,
							200,
							300,
							150,
							150
						)

						ButtonTable.DockWidget = plugin:CreateDockWidgetPluginGui("RobloxGPT", widgetInfo)
						ButtonTable.DockWidget.Title = "RobloxGPT"
						ButtonTable.GUI.Parent = ButtonTable.DockWidget

						ButtonTable.Connections.BindToCloseDockWidget = ButtonTable.DockWidget:BindToClose(ButtonTable.DockWidgetDestroyed)
					else
						ButtonTable.DockWidget.Enabled = true
					end
				elseif ButtonTable.DockWidget then
					ButtonTable.DockWidgetDestroyed()
				end
			end)
			
			local function WriteTheme()
				if not themes[settings.Mode] then return end
				ButtonTable.GUI.Chat.BackgroundColor3 = themes[settings.Mode].Primary
				ButtonTable.GUI.Conversations.Mode.BackgroundColor3 = themes[settings.Mode].Primary
				ButtonTable.GUI.Conversations.API.BackgroundColor3 = themes[settings.Mode].Primary
				ButtonTable.GUI.Conversations.API.Box.BackgroundColor3 = themes[settings.Mode].Primary
				ButtonTable.GUI.Conversations.NewChat.BackgroundColor3 = themes[settings.Mode].Primary
				ButtonTable.GUI.Conversations.ConversationTemplate.BackgroundColor3 = themes[settings.Mode].Primary
				
				ButtonTable.GUI.Conversations.BackgroundColor3 = themes[settings.Mode].Secondary
				ButtonTable.GUI.Chat.MessageBox.BackgroundColor3 = themes[settings.Mode].Secondary
				ButtonTable.GUI.Chat.ChatTemplate.BackgroundColor3 = themes[settings.Mode].Secondary
				
				ButtonTable.GUI.Conversations.Mode.Icon.ImageColor3 = themes[settings.Mode].Secondary
				ButtonTable.GUI.Conversations.Mode.ButtonText.TextColor3 = themes[settings.Mode].Tertiary
				
				ButtonTable.GUI.Conversations.API.ButtonText.TextColor3 = themes[settings.Mode].Tertiary
				ButtonTable.GUI.Conversations.API.Box.TextColor3 = themes[settings.Mode].Tertiary
				ButtonTable.GUI.Conversations.API.Box.PlaceholderColor3 = themes[settings.Mode].Tertiary
				
				ButtonTable.GUI.Conversations.NewChat.Icon.ImageColor3 = themes[settings.Mode].Secondary
				ButtonTable.GUI.Conversations.NewChat.ButtonText.TextColor3 = themes[settings.Mode].Tertiary

				ButtonTable.GUI.Conversations.ConversationTemplate.Icon.ImageColor3 = themes[settings.Mode].Secondary
				ButtonTable.GUI.Conversations.ConversationTemplate.ButtonText.TextColor3 = themes[settings.Mode].Tertiary
				
				ButtonTable.GUI.Chat.ChatTemplate.ButtonText.TextColor3 = themes[settings.Mode].Quaternary
				ButtonTable.GUI.Chat.MessageBox.TextColor3 = themes[settings.Mode].Quaternary
				
				for _, v in next, ButtonTable.GUI.Conversations.List:GetChildren() do
					if v:IsA("TextLabel") then
						v.BackgroundColor3 = themes[settings.Mode].Primary
						v.Icon.ImageColor3 = themes[settings.Mode].Secondary
						v.ButtonText.TextColor3 = themes[settings.Mode].Tertiary
					end
				end
				
				for _, v in next, ButtonTable.GUI.Chat.List:GetChildren() do
					if v:IsA("TextLabel") then
						v.BackgroundColor3 = themes[settings.Mode].Secondary
						v.ButtonText.TextColor3 = themes[settings.Mode].Quaternary
					end
				end
			end
			
			local LongItem = ButtonTable.GUI.Chat.LongItem
			local function LoadConversation(ConversationId, Thumbnail)
				LongItem.Parent = ButtonTable.GUI.Chat
				for _, v in next,  ButtonTable.GUI.Chat.List:GetChildren() do
					if v:IsA("TextLabel") then
						v:Destroy()
					end
				end
				
				local robloxGPTFrame = ButtonTable.GUI
				for i, v in next, settings.Conversations[ConversationId] do
					local chatTemplatePlayer = robloxGPTFrame.Chat.ChatTemplate:Clone()
					chatTemplatePlayer.Name = #robloxGPTFrame.Chat.List:GetChildren()
					chatTemplatePlayer.ButtonText.Text = v[2]
					chatTemplatePlayer.Parent = robloxGPTFrame.Chat.List
					chatTemplatePlayer.Icon.Image = v[1] == "Bot" and "rbxassetid://12539250892" or Thumbnail
					chatTemplatePlayer.Visible = true
				end; LongItem.Parent = ButtonTable.GUI.Chat.List
			end
			
			local function NewConversation(ConversationId)
				local ConversationId = ConversationId or "New Chat"
				if ConversationId == "New Chat" then
					if ButtonTable.GUI.Conversations.List:FindFirstChild(ConversationId) then
						local index = 0
						repeat
							index = index + 1
						until not ButtonTable.GUI.Conversations.List:FindFirstChild(ConversationId..index)
						ConversationId = ConversationId..index
					end
				end

				local Conversation = ButtonTable.GUI.Conversations.ConversationTemplate:Clone()
				Conversation.Name = ConversationId
				Conversation.Visible = true
				Conversation.ButtonText.Text = ConversationId
				Conversation.Parent = ButtonTable.GUI.Conversations.List

				Conversation.Trash.MouseButton1Click:Connect(function()
					if Writing then return end
					if settings.Conversations[ConversationId] then
						settings.Conversations[ConversationId] = nil
						Conversation:Destroy()
						if ConversationId == settings.CurrentConversation then
							for _, v in next,  ButtonTable.GUI.Chat.List:GetChildren() do
								if v:IsA("TextLabel") then
									v:Destroy()
								end
							end; settings.CurrentConversation = nil
						end
					end
				end)

				Conversation.Button.MouseButton1Down:Connect(function()
					if Writing then return end
					settings.CurrentConversation = ConversationId
					LoadConversation(ConversationId, Thumbnail)
				end)

				settings.Conversations[ConversationId] = settings.Conversations[ConversationId] or {}
				plugin:SetSetting("RobloxGPT", settings)

				return ConversationId
			end
			
			local function LoadConversations()
				for i, v in next, settings.Conversations do
					NewConversation(i)
					if i == settings.CurrentConversation then
						LoadConversation(i, Thumbnail)
					end
				end
			end
			
			WriteTheme()
			LoadConversations()
			ButtonTable.GUI.Conversations.Mode.Button.MouseButton1Down:Connect(function()
				settings.Mode = settings.Mode == "Dark" and "Light" or "Dark"
				ButtonTable.GUI.Conversations.Mode.ButtonText.Text = settings.Mode
				WriteTheme()
				plugin:SetSetting("RobloxGPT", settings)
			end)
			
			ButtonTable.GUI.Conversations.NewChat.Button.MouseButton1Down:Connect(function()
				settings.CurrentConversation = NewConversation()
			end)
			
			ButtonTable.GUI.Chat.MessageBox.TextEditable = true
			ButtonTable.GUI.Chat.MessageBox.FocusLost:Connect(function(entered)
				if entered then
					if not settings.CurrentConversation then
						settings.CurrentConversation = NewConversation()
					end
					local message = ButtonTable.GUI.Chat.MessageBox.Text
					ButtonTable.GUI.Chat.MessageBox.Text = ""
					if message:gsub("%s+", "") ~= "" then
						ButtonTable.GUI.Chat.MessageBox.TextEditable = false
						Writing = true
						LongItem.Parent = ButtonTable.GUI.Chat
						local response, messageSent = script.Parent.Message:InvokeServer(game.Players.LocalPlayer.UserId, settings.API, message, settings.CurrentConversation)
						Writing = false
						if messageSent then
							table.insert(settings.Conversations[settings.CurrentConversation], {"User", message})
							table.insert(settings.Conversations[settings.CurrentConversation], {"Bot", response.Response})
							plugin:SetSetting("RobloxGPT", settings)
						end; LongItem.Parent = ButtonTable.GUI.Chat.List
						ButtonTable.GUI.Chat.MessageBox.TextEditable = true
					end
				end
			end)
			
			ButtonTable.GUI.Conversations.API.Box.FocusLost:Connect(function()
				ButtonTable.GUI.Conversations.API.Box.Visible = false
				settings.API = ButtonTable.GUI.Conversations.API.Box.Text:gsub("%s+", "") 
				plugin:SetSetting("RobloxGPT", settings)
			end)
			
			ButtonTable.GUI.Conversations.API.Button.MouseButton1Down:Connect(function()
				ButtonTable.GUI.Conversations.API.Box.Visible = true
				ButtonTable.GUI.Conversations.API.Box.Text = settings.API or ""
				ButtonTable.GUI.Conversations.API.Box:CaptureFocus()
				while task.wait() and ButtonTable.GUI.Conversations.API.Box:IsFocused() do
					ButtonTable.GUI.Conversations.API.Box.CursorPosition = #string.split(ButtonTable.GUI.Conversations.API.Box.Text, "") + 1
				end
			end)
		end
	},
}

for _, v in next, PLUGIN.Buttons do
	v.StartUp(v)
end]]></ProtectedString>
