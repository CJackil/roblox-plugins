				<token name="RunContext">0</token>
				<string name="ScriptGuid">{77A32650-AF27-4CF9-9882-473E603D948E}</string>
				<ProtectedString name="Source"><![CDATA[local httpService = game:GetService("HttpService")
local robloxGPT = script.Parent:WaitForChild("RobloxGPT")
local robloxGPTFrame = robloxGPT:WaitForChild("RobloxGPT")

local debounce = 0
local cooldown = 6
local typingIndex = 0
local thinking = false
local writing = false

local Thumbnail

function BoundText(ChatTemplateBot, Bound)
	ChatTemplateBot.ButtonText.Text = ChatTemplateBot.ButtonText.Text..Bound
	if not ChatTemplateBot.ButtonText.TextFits then
		game:GetService("TweenService"):Create(ChatTemplateBot.ButtonText, TweenInfo.new(0.05, Enum.EasingStyle.Exponential), {
			Size = ChatTemplateBot.ButtonText.Size + UDim2.new(0, 0, 0, ChatTemplateBot.ButtonText.TextSize/4)
		}):Play()
	end return ChatTemplateBot.ButtonText.TextFits
end

local LongItem = robloxGPTFrame.Chat:FindFirstChild("LongItem") or robloxGPTFrame.Chat.List:FindFirstChild("LongItem")
script.Parent.Message.OnServerInvoke = function(_, userId, API, message, conversationId)
	if tick() - debounce < cooldown or not API or not message or not conversationId then return end
	debounce = tick()
	
	if not Thumbnail then
		Thumbnail = game:GetService("Players"):GetUserThumbnailAsync(userId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size100x100)
	end
	
	local chatTemplatePlayer = robloxGPTFrame.Chat.ChatTemplate:Clone()
	chatTemplatePlayer.Name = #robloxGPTFrame.Chat.List:GetChildren()
	chatTemplatePlayer.ButtonText.Text = message
	chatTemplatePlayer.Parent = robloxGPTFrame.Chat.List
	chatTemplatePlayer.Icon.Image = Thumbnail
	chatTemplatePlayer.Visible = true
	
	local chatTemplateBot = robloxGPTFrame.Chat.ChatTemplate:Clone()
	chatTemplateBot.Name = #robloxGPTFrame.Chat.List:GetChildren()
	chatTemplateBot.Parent = robloxGPTFrame.Chat.List
	chatTemplateBot.Icon.Image = "rbxassetid://12539250892"
	chatTemplateBot.Visible = true
	
	writing = false
	thinking = true
	local success, response, writeTextEnabled
	coroutine.resume(coroutine.create(function()
		local index = 0
		repeat task.wait(0.2)
			index = (index + 1)%4
			index = index == 0 and 1 or index
			local periods = ""
			for i = 1, index do periods = periods.."." end
			chatTemplateBot.ButtonText.Text = "Thinking"..periods	
		until response
		writeTextEnabled = true
	end))
	
	success, response = pcall(function()
		return game:GetService("HttpService"):JSONDecode(httpService:GetAsync("https://RobloxGPT.waiwaiperry.repl.co/chatgpt?API="..API.."&Message="..message.."&ConversationId=ROBLOXGPT"))
	end); repeat task.wait() until writeTextEnabled
	typingIndex = typingIndex + 1
	thinking = false
	writing = true
	
	local memoryTypingIndex = typingIndex
	if response and response.Response then
		chatTemplateBot.ButtonText.Text = ""
		for _, v in next, string.split(response.Response, " ") do
			if memoryTypingIndex ~= typingIndex then break end

			if math.random(1, 2) == 1 then
				for _, v in next, string.split(v, "") do
					BoundText(chatTemplateBot, v)
					
					if v == "." then
						task.wait(math.random() * 0.1)
					elseif v == "," then
						task.wait(math.random() * 0.1)
					else
						task.wait(((math.random() * math.random())^2)/(math.random() * 8 + 2))
					end
				end
				
				BoundText(chatTemplateBot, " ")
			else
				BoundText(chatTemplateBot, v.." ")
				task.wait()
			end
		end
	end
	
	if memoryTypingIndex == typingIndex then
		writing = false
	end
	
	return response, true
end]]></ProtectedString>
